            <Tooltip text="Send Typed message to the selected User." />
         </tooltip></Button>
      <TextArea fx:id="writeArea" layoutX="251.0" layoutY="283.0" prefHeight="111.0" prefWidth="231.0" promptText="Write your message here. :)">
