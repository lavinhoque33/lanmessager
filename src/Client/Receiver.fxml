         <font>
            <Font size="10.0" />
         </font>
         <tooltip>
            <Tooltip text="Clear Log Box" />
         </tooltip>
      </Button>
      <Button fx:id="chatclear" layoutX="735.0" layoutY="31.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#cclearaction" prefHeight="20.0" prefWidth="20.0" text="C">
         <font>
            <Font size="9.0" />
         </font>
         <tooltip>
            <Tooltip text="Clear Chat Box">
               <font>
                  <Font name="Comic Sans MS" size="18.0" />
               </font>
            </Tooltip>
         </tooltip>
      </Button>
      <Button fx:id="blockbutton" layoutX="155.0" layoutY="12.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#blockaction" prefHeight="39.0" prefWidth="67.0" text="Block" textFill="#da3939">
         <tooltip>
            <Tooltip text="Block the Selected User.You won't get any more message from him." />
         </tooltip></Button>
      <Button fx:id="blocklistbutton" layoutX="22.0" layoutY="12.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#blistaction" prefHeight="37.0" prefWidth="87.0" text="BlockList" textFill="#a5a863">
